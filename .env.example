# Environment Configuration Template
# Copy this file to .env.development or .env.production and fill in the values

# Database - SQLite database file path (relative to prisma/ directory)
# Use "../" prefix to store database in project root
DATABASE_URL="file:../finance.db"

# Plaid Configuration
PLAID_CLIENT_ID=your_client_id
PLAID_SECRET=your_secret
PLAID_ENV=sandbox  # Use "sandbox" for development, "production" for prod

# Next.js
NEXT_PUBLIC_APP_URL=http://localhost:3000

# OpenAI
OPENAI_API_KEY=sk
